   "source": [
    "def fibo(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    elif n == 0 :\n",
    "        return 0\n",
    "    else:  return fibo(n-1)+fibo(n-2)\n",
    "        \n",
    "result = fibo(4)\n",
    "print(result)"
   ]
